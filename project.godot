config_version=5

[application]

config/name="What Remains in Sleep"
config/description="What Remains in Sleep is a 2D narrative puzzle-horror game made in Godot. You wake up in a strange, looping place with no memory of how you got there. Something is wrong. Time is stuck. The walls remember things you do not."
run/main_scene="res://scenes/Menus/StartMenu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://Sprites&TileMaps/icon/WhatRemainsInSleep.png"

[autoload]

GlobalInput="*res://scripts/GlobalInput.gd"
GlobalProgress="*res://scripts/GlobalProgress.gd"

[display]

window/size/viewport_width=288
window/size/viewport_height=192
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"
window/main_scene="res://scenes/StartMenu.tscn"

[gui]

theme/custom_font="res://fonts/QuinqueFive_Font_1_15/QuinqueFive.otf"

